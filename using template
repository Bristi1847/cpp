#include<iostream>
using namespace std;


template <typename T>
T add(T a, T b) {
    return a + b;
}

int main() {
    cout << "Roll No: 24B11AI397\n";

    cout << "Sum of integers: " << add(5, 10) << endl;
    cout << "Sum of floats: " << add(3.5f, 2.5f) << endl;
    cout << "Sum of doubles: " << add(4.2, 6.8) << endl;

    return 0;
}
